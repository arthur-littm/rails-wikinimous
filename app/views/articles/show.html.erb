<div class="container text-center">
  <h4><%= link_to("Edit", edit_article_path(@article)) %> <%= link_to("Delete", article_path(@article), method: :delete) %></h4>
  <h1><%= @article.title %> </h1>
</div>
<div class="container text-center">
  <img src="http://lorempixel.com/800/300/city/" class="container-fluid">
  <%= @article.content.html_safe %>
  <h4 style="margin-top: 40px"><%= link_to("Back to articles", articles_path) %></h4>
</div>
